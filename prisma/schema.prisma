generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id     Int    @id @default(autoincrement())
  nombre String
  email  String @unique
}

model Carros {
  id            Int    @id @default(autoincrement())
  marca         String
  NumeroPlacas  String @unique
}